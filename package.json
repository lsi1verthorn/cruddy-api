{
  "name": "cruddy-api",
  "version": "1.0.0",
  "description": "Test CRUD api for rudimentary postgres db",
  "main": "app.js",
  "scripts": {
    "build": "pnpm install",
    "dev": "node app.js",
    "lint": "eslint .",
    "prepare": "husky install",
    "test": "jest --testMatch \"**/*.test.[jt]s\"",
    "test:app": "mocha __tests__/app.test.js"
  },
  "keywords": [
    "CRUD api",
    "express",
    "postgres"
  ],
  "author": "Linda Silverthorn",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.8.4",
    "body-parser": "^2.2.0",
    "express": "^5.1.0",
    "jest": "^29.7.0",
    "mocha": "^11.1.0",
    "pg": "^8.14.1",
    "pnpm": "^10.8.1",
    "semver": "^7.7.1",
    "sequelize": "^6.37.7",
    "sqlite3": "^5.1.7",
    "supertest": "^7.1.0"
  },
  "pnpm": {
    "ignoredBuiltDependencies": [
      "sqlite3"
    ],
    "onlyBuiltDependencies": [
      "sqlite3"
    ]
  },
  "devDependencies": {
    "eslint": "^9.24.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-node": "^11.1.0",
    "husky": "^8.0.3"
  }
}
